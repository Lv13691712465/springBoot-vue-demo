{"ast":null,"code":"// @ is an alias to /src\nimport request from \"../../utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res);\n        this.tableData = res.data.data.search;\n      });\n    },\n\n    handleEdit() {},\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {},\n\n    handleDelete() {}\n\n  }\n};","map":{"version":3,"mappings":"AAkFA;AAEA,OAAOA,OAAP,MAAoB,qBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAKbC,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAC,EADD;MAEJC,aAAa,EAAC,KAFV;MAGJC,MAAM,EAAC,EAHH;MAIJC,KAAK,EAAC,CAJF;MAKJC,WAAW,EAAC,CALR;MAMJC,QAAQ,EAAC,EANL;MAOJC,SAAS,EAAC;IAPN,CAAN;EASD,CAfY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAlBY;;EAmBbC,OAAO,EAAC;IACJD,IAAI,GAAE;MACJZ,OAAO,CAACc,GAAR,CAAY,OAAZ,EAAoB;QAClBC,MAAM,EAAC;UACLC,OAAO,EAAC,KAAKR,WADR;UAELC,QAAQ,EAAC,KAAKA,QAFT;UAGLH,MAAM,EAAC,KAAKA;QAHP;MADW,CAApB,EAMGW,IANH,CAMQC,GAAE,IAAI;QACZC,OAAO,CAACC,GAAR,CAAYF,GAAZ;MACD,CARD;IASH,CAXK;;IAYNG,GAAG,GAAE;MACH,KAAKhB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAfK;;IAiBNkB,IAAI,GAAE;MACJtB,OAAO,CAACuB,IAAR,CAAa,OAAb,EAAqB,KAAKnB,IAA1B,EAAgCa,IAAhC,CAAqCC,GAAE,IAAK;QAC1CC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKR,SAAL,GAAiBQ,GAAG,CAACf,IAAJ,CAASA,IAAT,CAAcG,MAA/B;MACD,CAHD;IAID,CAtBK;;IAwBNkB,UAAU,GAAE,CACX,CAzBK;;IA2BNC,gBAAgB,GAAE,CACjB,CA5BK;;IA8BNC,mBAAmB,GAAE,CACpB,CA/BK;;IAiCNC,YAAY,GAAE,CACb;;EAlCK;AAnBK,CAAf","names":["request","name","components","data","form","dialogVisible","search","total","currentPage","pageSize","tableData","created","load","methods","get","params","pageNum","then","res","console","log","add","save","post","handleEdit","handleSizeChange","handleCurrentChange","handleDelete"],"sourceRoot":"","sources":["D:\\javaCode\\springBoot+vue\\springboot-vue-demo\\vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n    <div style=\"padding: 10px\">\n\n        <div style = \"margin: 10px\">\n          <el-button type = \"primary\" @click=\"add\">新增</el-button>\n          <el-button type = \"primary\">导入</el-button>\n          <el-button type = \"primary\">导出</el-button>\n        </div>\n\n        <div style=\"margin: 10px 0\">\n          <el-input v-model=\"search\" placeholder=\"请输入关键字\" style = \"width: 20%\"></el-input>\n          <el-button type=\"primary\" style =\"margin-left:5px\">查询</el-button>\n        </div>\n        <el-table :data=\"tableData\"\n                  border\n                  stripe\n                  style=\"width: 100%\">\n          <el-table-column prop=\"id\" label=\"ID\" sortable/>\n          <el-table-column prop=\"userName\" label=\"用户名\"/>\n          <el-table-column prop=\"nickName\" label=\"昵称\" />\n          <el-table-column prop=\"age\" label=\"年龄\" />\n          <el-table-column prop=\"sex\" label=\"性别\" />\n          <el-table-column prop=\"address\" label=\"地址\" />\n          <el-table-column fixed=\"right\" label=\"Operations\">\n            <template #default>\n              <el-button size=\"mini\" @click=\"handleEdit(scope.row)\"\n              >编辑</el-button>\n              <el-popconfirm\n                  confirm-button-text=\"OK\"\n                  cancel-button-text=\"No, Thanks\"\n                  :icon=\"InfoFilled\"\n                  icon-color=\"#626AEF\"\n                  title=\"Are you sure to delete this?\"\n              >\n                <template #reference>\n                  <el-button size=\"mini\" type=\"danger\" @click=\"handleDelete(scope.$index,scope.row)\">删除</el-button>\n                </template>\n              </el-popconfirm>\n            </template>\n          </el-table-column>\n        </el-table>\n        <div style=\"margin:10px 0\">\n          <el-pagination\n            @size-change=\"handleSizeChange\"\n            @current-change=\"handleCurrentChange\"\n            :currentPage=\"currentPage\"\n            :page-size=\"pageSize\"\n            :page-sizes=\"[5, 10, 20]\"\n            layout=\"total, sizes, prev, pager, next, jumper\"\n            :total=\"total\">\n          </el-pagination>\n          <el-dialog title=\"提示\" v-model=\"dialogVisible\" width=\"30%\">\n            <el-form :model = \"form\" label-width=\"120px\">\n              <el-form-item label=\"用户名\">\n                <el-input v-model=\"form.username\" style = \"width:80%\"></el-input>\n              </el-form-item>\n              <el-form-item label=\"昵称\">\n                <el-input v-model=\"form.nickName\" style = \"width:80%\"></el-input>\n              </el-form-item>\n              <el-form-item label=\"年龄\">\n                <el-input v-model=\"form.age\" style = \"width:80%\"></el-input>\n              </el-form-item>\n              <el-form-item label=\"性别\">\n                <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\n                <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\n                <el-radio v-model=\"form.sex\" label=\"未知\">未知</el-radio>\n              </el-form-item>\n              <el-form-item label=\"地址\">\n                <el-input v-model=\"form.address\" style = \"width:80%\"></el-input>\n              </el-form-item>\n            </el-form>\n            <template #footer>\n                <span class=\"dialog-footer\">\n                  <el-button @click=\"dialogVisible = false\">取 消</el-button>\n                  <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n                </span>\n            </template>\n          </el-dialog>\n        </div>\n    </div>\n</template>\n<script>\n// @ is an alias to /src\n\nimport request from \"../../utils/request\";\n\nexport default {\n  name: 'HomeView',\n  components: {\n\n  },\n  data(){\n    return{\n      form:{},\n      dialogVisible:false,\n      search:'',\n      total:0,\n      currentPage:1,\n      pageSize:10,\n      tableData:[]\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods:{\n      load(){\n        request.get(\"/user\",{\n          params:{\n            pageNum:this.currentPage,\n            pageSize:this.pageSize,\n            search:this.search\n          }\n        }).then(res =>{\n          console.log(res)\n        })\n    },\n    add(){\n      this.dialogVisible = true;\n      this.form = {}\n    },\n\n    save(){\n      request.post(\"/user\",this.form).then(res => {\n        console.log(res)\n        this.tableData = res.data.data.search;\n      })\n    },\n\n    handleEdit(){\n    },\n\n    handleSizeChange(){\n    },\n\n    handleCurrentChange(){\n    },\n\n    handleDelete(){\n    },\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}