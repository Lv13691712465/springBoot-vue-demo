{"ast":null,"code":"// @ is an alias to /src\nimport request from \"../../utils/request\";\nexport default {\n  name: 'HomeView',\n  components: {},\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: '',\n      total: 0,\n      currentPage: 1,\n      pageSize: 10,\n      tableData: []\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //刷新表格的数据\n\n          this.dialogVisible = false;\n        });\n      } else {\n        //新增\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n\n          this.load(); //删除表格的数据\n\n          this.dialogVisible = false; //关闭弹窗\n        });\n      }\n\n      request.post(\"/user\", this.form).then(res => {\n        console.log(res);\n      });\n    },\n\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      this.currentPage = pageNum;\n      this.load();\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        console.log(res);\n\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiFA;AAEA,OAAOA,OAAP,MAAoB,qBAApB;AAEA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE,EADD;MAELC,aAAa,EAAE,KAFV;MAGLC,MAAM,EAAE,EAHH;MAILC,KAAK,EAAE,CAJF;MAKLC,WAAW,EAAE,CALR;MAMLC,QAAQ,EAAE,EANL;MAOLC,SAAS,EAAE;IAPN,CAAP;EASD,CAbY;;EAcbC,OAAO,GAAG;IACR,KAAKC,IAAL;EACD,CAhBY;;EAiBbC,OAAO,EAAE;IACPD,IAAI,GAAG;MACLZ,OAAO,CAACc,GAAR,CAAY,OAAZ,EAAqB;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,KAAKR,WADR;UAENC,QAAQ,EAAE,KAAKA,QAFT;UAGNH,MAAM,EAAE,KAAKA;QAHP;MADW,CAArB,EAMGW,IANH,CAMQC,GAAE,IAAK;QACbC,OAAO,CAACC,GAAR,CAAYF,GAAZ;QACA,KAAKR,SAAL,GAAiBQ,GAAG,CAACf,IAAJ,CAASkB,OAA1B;QACA,KAAKd,KAAL,GAAaW,GAAG,CAACf,IAAJ,CAASI,KAAtB;MACD,CAVD;IAWD,CAbM;;IAcPe,GAAG,GAAG;MACJ,KAAKjB,aAAL,GAAqB,IAArB;MACA,KAAKD,IAAL,GAAY,EAAZ;IACD,CAjBM;;IAmBPmB,UAAU,CAACC,GAAD,EAAM;MACd,KAAKpB,IAAL,GAAYqB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAZ;MACA,KAAKnB,aAAL,GAAqB,IAArB;IACD,CAtBM;;IAwBPuB,IAAI,GAAG;MACL,IAAI,KAAKxB,IAAL,CAAUyB,EAAd,EAAkB;QAAE;QAClB7B,OAAO,CAAC8B,GAAR,CAAY,OAAZ,EAAqB,KAAK1B,IAA1B,EAAgCa,IAAhC,CAAqCC,GAAE,IAAK;UAC1CC,OAAO,CAACC,GAAR,CAAYF,GAAZ;;UACA,IAAIA,GAAG,CAACa,IAAJ,IAAY,GAAhB,EAAqB;YACnB,KAAKC,QAAL,CAAc;cACZC,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CAAd;UAIF,CALA,MAKO;Y